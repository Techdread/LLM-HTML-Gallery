{
  "title": "3D Lennard-Jones Fluid/Gas Simulation",
  "htmlContent": "<!DOCTYPE html>\r\n<html lang=\"en\">\r\n<head>\r\n    <meta charset=\"UTF-8\">\r\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\r\n    <title>3D Lennard-Jones Fluid/Gas Simulation</title>\r\n    <link rel=\"stylesheet\" href=\"style.css\">\r\n    <script src=\"https://cdn.jsdelivr.net/npm/three@0.132.2/build/three.min.js\"></script>\r\n    <script src=\"https://cdn.jsdelivr.net/npm/three@0.132.2/examples/js/controls/OrbitControls.js\"></script>\r\n</head>\r\n<body>\r\n    <div class=\"container\">\r\n        <div id=\"simulation-container\"></div>\r\n        <div class=\"controls-panel\">\r\n            <h1>Lennard-Jones Simulation</h1>\r\n            \r\n            <div class=\"control-group\">\r\n                <h2>Simulation Parameters</h2>\r\n                <div class=\"control\">\r\n                    <label for=\"particleCount\">Particle Count:</label>\r\n                    <input type=\"range\" id=\"particleCount\" min=\"10\" max=\"500\" value=\"100\">\r\n                    <span id=\"particleCountValue\">100</span>\r\n                </div>\r\n                <div class=\"control\">\r\n                    <label for=\"boxSize\">Box Size:</label>\r\n                    <input type=\"range\" id=\"boxSize\" min=\"5\" max=\"20\" step=\"0.5\" value=\"10\">\r\n                    <span id=\"boxSizeValue\">10</span>\r\n                </div>\r\n                <div class=\"control\">\r\n                    <label for=\"timeStep\">Time Step (dt):</label>\r\n                    <input type=\"range\" id=\"timeStep\" min=\"0.001\" max=\"0.05\" step=\"0.001\" value=\"0.01\">\r\n                    <span id=\"timeStepValue\">0.01</span>\r\n                </div>\r\n            </div>\r\n            \r\n            <div class=\"control-group\">\r\n                <h2>Lennard-Jones Parameters</h2>\r\n                <div class=\"control\">\r\n                    <label for=\"epsilon\">Epsilon (ε):</label>\r\n                    <input type=\"range\" id=\"epsilon\" min=\"0.1\" max=\"2\" step=\"0.1\" value=\"1\">\r\n                    <span id=\"epsilonValue\">1</span>\r\n                </div>\r\n                <div class=\"control\">\r\n                    <label for=\"sigma\">Sigma (σ):</label>\r\n                    <input type=\"range\" id=\"sigma\" min=\"0.5\" max=\"2\" step=\"0.1\" value=\"1\">\r\n                    <span id=\"sigmaValue\">1</span>\r\n                </div>\r\n                <div class=\"control\">\r\n                    <label for=\"cutoffRadius\">Cutoff Radius:</label>\r\n                    <input type=\"range\" id=\"cutoffRadius\" min=\"2\" max=\"5\" step=\"0.1\" value=\"3\">\r\n                    <span id=\"cutoffRadiusValue\">3</span>\r\n                </div>\r\n            </div>\r\n            \r\n            <div class=\"control-group\">\r\n                <h2>Temperature Control</h2>\r\n                <div class=\"control\">\r\n                    <label for=\"initialTemp\">Initial Temperature:</label>\r\n                    <input type=\"range\" id=\"initialTemp\" min=\"0.1\" max=\"5\" step=\"0.1\" value=\"1\">\r\n                    <span id=\"initialTempValue\">1</span>\r\n                </div>\r\n                <div class=\"control\">\r\n                    <label for=\"targetTemp\">Target Temperature:</label>\r\n                    <input type=\"range\" id=\"targetTemp\" min=\"0.1\" max=\"5\" step=\"0.1\" value=\"1\">\r\n                    <span id=\"targetTempValue\">1</span>\r\n                </div>\r\n                <div class=\"control\">\r\n                    <label for=\"thermostatEnabled\">Use Thermostat:</label>\r\n                    <input type=\"checkbox\" id=\"thermostatEnabled\" checked>\r\n                </div>\r\n            </div>\r\n            \r\n            <div class=\"buttons\">\r\n                <button id=\"startBtn\">Start</button>\r\n                <button id=\"pauseBtn\">Pause</button>\r\n                <button id=\"resetBtn\">Reset</button>\r\n            </div>\r\n            \r\n            <div class=\"stats\">\r\n                <h2>Simulation Statistics</h2>\r\n                <div>Current Temperature: <span id=\"currentTemp\">0</span></div>\r\n                <div>Kinetic Energy: <span id=\"kineticEnergy\">0</span></div>\r\n                <div>Potential Energy: <span id=\"potentialEnergy\">0</span></div>\r\n                <div>Total Energy: <span id=\"totalEnergy\">0</span></div>\r\n                <div>Elapsed Time: <span id=\"elapsedTime\">0</span></div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n\r\n    <script src=\"simulation.js\"></script>\r\n</body>\r\n</html>",
